#include<iostream>
using namespace std;
class matrix
{
    int a[5][5];

public:

    matrix()
    {
        for(int i=0;i<5;i++)
            for(int j=0;j<5;j++)
                a[i][j]=0;
    }
    matrix(int n)
    {
        for(int i=0;i<5;i++)
            for(int j=0;j<5;j++)
                a[i][j]=n;
    }

    matrix operator =(matrix b)
    {
        for(int i=0;i<5;i++)
            for(int j=0;j<5;j++)
                b[i][j]=b.a[i][j];

        return *this;
    }

    void Display()
    {
        for(int i=0;i<5;i++)
           {

            for(int j=0;j<5;j++)
               {

                cout<<a[i][j]<<" ";
               }
               cout<<endl;
           }

    }
};

int main()
{
    matrix A(5);
        A.Display();
    matrix B;

    B.Display();

    B=A;
    B.Display();
    
    return 0;
}

